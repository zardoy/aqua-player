    "@types/ws": "^8.18.1",
    "change-case": "^5.4.4",
    "electron-log": "^5.4.2",
    "electron-squirrel-startup": "^1.0.1",
    "electron-updater": "^6.3.9",
    "electron-window-keeper": "^0.0.2",
    "esbuild-electron": "^0.1.4",
    "esbuild-loader": "^4.3.0",
    "ffmpeg-static": "^5.2.0",
    "ffprobe-static": "^3.1.0",
